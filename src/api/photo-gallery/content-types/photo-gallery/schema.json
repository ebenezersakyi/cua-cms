{
  "kind": "collectionType",
  "collectionName": "photo_galleries",
  "info": {
    "singularName": "photo-gallery",
    "pluralName": "photo-galleries",
    "displayName": "Photo Gallery",
    "description": "Photo collections and albums"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "albumName": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "albumName",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": ["images"]
    },
    "event": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event"
    },
    "date": {
      "type": "date"
    },
    "photographer": {
      "type": "string"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag"
    }
  }
}
