{
  "kind": "collectionType",
  "collectionName": "testimonial_members",
  "info": {
    "singularName": "testimonial-member",
    "pluralName": "testimonial-members",
    "displayName": "Testimonial Member",
    "description": "Member who provided a testimonial"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "role": {
      "type": "string"
    },
    "location": {
      "type": "string",
      "required": true
    },
    "memberSince": {
      "type": "string",
      "required": true
    },
    "testimonial": {
      "type": "richtext",
      "required": true
    },
    "rating": {
      "type": "integer",
      "min": 1,
      "max": 5,
      "default": 5
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "creditUnion": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::credit-union.credit-union",
      "inversedBy": "testimonials"
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}
