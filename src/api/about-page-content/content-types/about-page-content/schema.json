    "historyTitle": {
      "type": "string",
      "default": "Our History"
    },
    "historyContent": {
      "type": "richtext"
    },
    "establishedYear": {
      "type": "integer",
      "default": 1968
    },
    "missionTitle": {
      "type": "string",
      "default": "Our Mission"
    },
    "missionStatement": {
      "type": "richtext",
      "required": false
    },
    "visionTitle": {
      "type": "string",
      "default": "Our Vision"
    },
    "visionStatement": {
      "type": "richtext",
      "required": false
    },
    "valuesTitle": {
      "type": "string",
      "default": "Our Core Values"
    },
    "valuesDescription": {
      "type": "text"
    },
    "coreValues": {
      "type": "component",
      "repeatable": true,
      "component": "shared.core-value",
