    "roleInGhanaTitle": {
      "type": "string",
      "default": "Our Role in Ghana"
    },
    "roleInGhanaContent": {
      "type": "richtext"
    },
    "potentialAndSizeTitle": {
      "type": "string",
      "default": "Potential & Size"
    },
    "potentialAndSizeContent": {
      "type": "richtext"
    },
    "totalCreditUnions": {
      "type": "integer"
    },
    "totalMembers": {
      "type": "integer"
    },
    "totalAssets": {
      "type": "string"
    },
    "ctaTitle": {
      "type": "string",
      "default": "Join the Movement"
    },
    "ctaDescription": {
      "type": "text"
    },
    "ctaPrimaryText": {
      "type": "string",
      "default": "Become a Member"
    },
    "ctaPrimaryLink": {
      "type": "string",
      "default": "/credit-unions/join"
    },
    "ctaSecondaryText": {
      "type": "string",
      "default": "Find Credit Unions"
    },
    "ctaSecondaryLink": {
      "type": "string",
      "default": "/credit-unions"
    },
    "seoTitle": {
      "type": "string"
    },
    "seoDescription": {
      "type": "text",
      "maxLength": 160
    }
  }
