      "allowedTypes": [
        "images"
      ]
    },
    "board_section_title": {
      "type": "string",
      "required": true,
      "default": "Experienced Leadership"
    },
    "board_section_subtitle": {
      "type": "text",
      "required": true,
      "maxLength": 300
    },
    "board_members": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::board-member.board-member"
